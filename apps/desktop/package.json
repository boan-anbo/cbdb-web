{
  "name": "cbdb-desktop",
  "productName": "CBDB Desktop",
  "version": "1.0.0",
  "description": "CBDB Desktop Application",
  "main": "dist/main/index.js",
  "scripts": {
    "clean": "rimraf dist node_modules/.vite",
    "dev": "npm run clean && vite",
    "dev:ui": "npm run clean && VITE_UI_ONLY=true vite",
    "dev:server": "CBDB_PATH=../../cbdb_sql_db/latest.db npm run dev",
    "dev:no-database": "npm run clean && CBDB_PATH=/nonexistent/database.db SKIP_DB_CHECK=true vite",
    "build": "npm run clean && vite build",
    "build:web": "npm run clean && VITE_UI_ONLY=true vite build --mode production",
    "build:dir": "npm run clean && vite build -- --electron-builder-options='--dir'",
    "build:dist": "npm run clean && vite build",
    "build:release": "npm run clean && vite build -- --electron-builder-options='--publish=never'",
    "build:workers": "tsx scripts/compile-workers.ts",
    "build:workers:watch": "tsx scripts/compile-workers.ts --watch",
    "dev:workers": "npm run build:workers:watch",
    "test:dir": "CBDB_PATH=../../cbdb_sql_db/latest.db APP_DB_PATH=/tmp/cbdb-desktop-app.db open dist/electron/mac-arm64/desktop.app",
    "debug:built": "./scripts/debug-built-app.sh",
    "lint": "eslint --ext .ts,.tsx .",
    "test": "vitest --project=storybook",
    "test:regular": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "storybook": "storybook dev -p 6006",
    "storybook:server": "cd ../server && CBDB_PATH=../../cbdb_sql_db/latest.db npm run start:dev",
    "storybook:full": "concurrently \"npm run storybook:server\" \"npm run storybook\"",
    "build-storybook": "storybook build",
    "postinstall": "electron-builder install-app-deps"
  },
  "keywords": [],
  "author": {
    "name": "Bo An",
    "email": "bo.an@aya.yale.edu"
  },
